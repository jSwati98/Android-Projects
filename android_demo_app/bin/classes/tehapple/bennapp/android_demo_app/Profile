public class Profile{
  private int id;
  private String  firstname;
  private String  picture;
  
  public Profile(int id, String fn){  
    this.id = id;
    this.firstname = fn;
  }

  public Profile(int id, String fn, String pic){
    this.id = id;
    this.firstname = fn;
    this.picture = pic;
  }

  public void setPicture(String pic){
    this.picture = pic;
  }

  public int getID(){
    return this.id;
  }

  public String getFirstName(){
    return this.firstname;
  }

  public String getPicture(){
    return this.picture;
  }
}
